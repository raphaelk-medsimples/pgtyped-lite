{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    },
    {
      "mode": "ts",
      "include": "**/*.ts",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    }
  ],
  "typesOverrides": {
    "date": {
      "return": "string"
    },
    "int8": "BigInt",
    "category": { "return": "./src/customTypes.js#Category" }
  },
  "srcDir": "./src/",
  "migrationsFile": "/home/issamuk/code/pgtyped/packages/example/sql/schema.sql"
}
