{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    },
    {
      "mode": "ts",
      "include": "**/*.ts",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    },
    {
      "mode": "ts-implicit",
      "include": "index.test.ts",
      "functionName": "sql",
      "emitFileName": "./src/sql/index.ts"
    }
  ],
  "typesOverrides": {
    "date": {
      "return": "string"
    },
    "int8": "BigInt",
    "category": { "return": "./src/customTypes.js#Category" }
  },
  "srcDir": "./src/",
  "dbUrl": "postgresql://neondb_owner:vzBnReJA7o2G@ep-soft-bird-a5iqel4j.us-east-2.aws.neon.tech/neondb?sslmode=require"
}
